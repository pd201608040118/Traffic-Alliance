<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.StudentDao" >
    <select id="showall" parameterType="com.example.demo.dao.StudentDao" resultType="com.example.demo.dao.domain.StudentDomain">
        select * from student
    </select>
    <select id="findByStudentName" parameterType="com.example.demo.dao.StudentDao" resultType="com.example.demo.dao.domain.StudentDomain">
        select StuName,Password from student where StuName=#{stuname}
    </select>
</mapper>